{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
        "no-console": "off",
        "no-tabs": ["error", { "allowIndentationTabs": true }],
        "no-mixed-spaces-and-tabs": 0,
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    }
  }